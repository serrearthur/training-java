<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="CustomCheckstyle" location="checkstyle/checkstyle.xml" type="project" description="">
    <property name="header.file" value="checkstyle/header.txt"/>
    <property name="suppressions.file" value="checkstyle/suppressions.xml"/>
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="CustomCheckstyle" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
</fileset-config>
